[module]
  [module.hugoVersion]
    extended = true
    min = "0.62.0"

[params]
  defaultTheme = "auto"
  gitRepo = ""
  fingerprint = ""
  dateFormat = "2006-01-02"
  title = "My cool site"
  description = "This is my cool site"
  images = []

  [params.header]
    desktopMode = "fixed"
    mobileMode = "auto"
    [params.header.title]
      logo = ""
      name = "My cool site"
      pre = ""
      post = ""
      typeit = false

  [params.footer]
    enable = true
    custom = ""
